journal = {Fuel},
keywords = {Coal,Coalbed methane,ECGR},
number = {10-11},
pages = {1601--1613},
title = {{Determination of multi-component gas and water equilibrium and non-equilibrium sorption isotherms in carbonaceous solids from early-time measurements}},
volume = {86},
year = {2007}
}
