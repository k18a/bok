@article{Bering,
abstract = {Adsorption on a nonporous, nonswelling adsorbent and adsorption in micropores represent two limiting cases of physical adsorption which are dearly distinguishable thermodynamieMly. In the first ease, the chemical potential of the adsorbent is inde-pendent of the amount of the adsorbed substance; in the second ease, it varies with it. The works of Brunauer, which made a whole epoch in the theory of adsorption, belong to the first type of adsorption phenomena; adsorption in micropores, however, is beyond the framework of his investigations. It should be treated in terms of volume filling, and not of layer-by-layer coverage of the surface. In computational methods of the theory in hand, a substantial role is played by the concept of the temperature invarianee of free energy of adsorption as a function of the degree of filling. An analysis of this concept is given and the limits of its ap-plicability are assessed. Exact and approximate expressions for the calculation of the basic thermody-namic functions in adsorption in micropores have been obtained. A generaliT.ed thermal equation of adsorption, i.e., {\~{}}(0, p, T) = 0, applicable to the quantitative description of adsorption of molecules sharply differing in electronic structure on microporous adsorbents widely different in nature (active carbons, zeolites, etc.) is considered.},
author = {Bering, B P and Dubinin, M M and Serpinsky, V V},
title = {{On Thermodynamics of Adsorption in Micropores}},
url = {https://ac-els-cdn-com.ezproxy.is.ed.ac.uk/0021979772902330/1-s2.0-0021979772902330-main.pdf?{\_}tid=e0d5dbcc-f132-46d3-b5e9-1546f32de81c{\&}acdnat=1522160999{\_}bc451790b5efb17a7f3ea1befea6dce8}
